{
  "name": "Stay Focused!!!!",
  "version": "1.0",
  "description": "Sensei I really really want to stay focused!",
  "permissions": ["webRequest", "webRequestBlocking",
                  "<all_urls>"],
  "background": {
    "scripts": ["url.js", "background.js"]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "16": "images/good16.jpg",
      "32": "images/good32.jpg",
      "48": "images/good48.jpg",
      "128": "images/good128.jpg"
    },
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
